{
    "git.path": "C:\\Users\\Etscheidt\\Documents\\prog\\github\bin\\git.exe",
    "command-runner.terminal.name": "runCommand",
    "command-runner.terminal.autoClear": true,
    "command-runner.terminal.autoFocus": true,
    "command-runner.commands": {

        "echo workspaceFolder": "echo ${workspaceFolder}",
        "Sphinx links" : "python -m sphinx.ext.intersphinx build/html/objects.inv "
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "git.enableSmartCommit": true,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
    "autoDocstring.docstringFormat": "sphinx",
    "editor.renderWhitespace": "all",
    "autoDocstring.quoteStyle": "'''",
    "pylint.args": ["--extension-pkg-whitelist=wx"],
}